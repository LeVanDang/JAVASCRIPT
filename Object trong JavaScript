// KHOI TAO OBJECT
// Khoi tao toàn bộ các trường và hàm
// Cach1:
var person = {
firstName : 'dang',
lastName : 'lv',
showName : function(){
console.log(this.firstName+'-'+this.lastName);
}
};

// Cach2: khoi tạo contructor

var person = new Object();
person.firstName = 'danglv';
person.lastName = '11234';
person.showName = function() {
console.log(this.firstName +'-'+this.lastName);
};
